{
  "version": 2,
  "outputCapture": "direct-nodejs",
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "cli": "nx",
  "title": "Deploy executor",
  "description": "Deploy",
  "properties": {
    "file": {
      "type": "string",
      "description": "What file to deploy"
    },
    "project": {
      "type": "string",
      "description": "Project id to deploy to"
    },
    "deletePolicy": {
      "type": "string",
      "default": "delete",
      "enum": ["abandon", "delete"]
    },
    "createPolicy": {
      "type": "string",
      "default": "create-or-acquire",
      "enum": ["acquire", "create-or-acquire"]
    },
    "preview": {
      "type": "boolean",
      "default": false
    }
  }
}
